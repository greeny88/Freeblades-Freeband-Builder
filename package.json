{
  "name": "freeblades-freeband-builder",
  "version": "3.0.0",
  "description": "Tool to construct a Freeblades freeband",
  "main": "index.html",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test-coverage": "ng test --code-coverage",
    "start-sw": "ng build && http-server dist",
    "ghpages-old": "ng build --base-href /Freeblades-Freeband-Builder/ && copy dist\\index.html dist\\404.html",
    "ghpages": "ng build --base-href /Freeblades-Freeband-Builder/",
    "ghpages-test": "ng test --no-watch --no-progress --browsers=ChromeHeadless"
  },
  "author": "mgreen",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/greeny88/Freeblades-Freeband-Builder.git"
  },
  "dependencies": {
    "@angular/animations": "^18.2.14",
    "@angular/cdk": "^18.2.14",
    "@angular/common": "^18.2.14",
    "@angular/compiler": "^18.2.14",
    "@angular/core": "^18.2.14",
    "@angular/forms": "^18.2.14",
    "@angular/material": "^18.2.14",
    "@angular/platform-browser": "^18.2.14",
    "@angular/platform-browser-dynamic": "^18.2.14",
    "@angular/router": "^18.2.14",
    "@angular/service-worker": "^18.2.14",
    "@tensorflow/tfjs": "^4.19.0",
    "material-components-web": "^14.0.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.21",
    "@angular/cli": "^18.2.21",
    "@angular/compiler-cli": "^18.2.14",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "autoprefixer": "^10.4.7",
    "http-server": "^14.1.1",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.0.24",
    "typescript": "~5.4.5"
  }
